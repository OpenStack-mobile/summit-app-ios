	pod 'Realm'
	pod 'RealmSwift'
	pod 'SwiftyJSON'
	pod 'HanekeSwift'
	pod 'AeroGearHttp', :git => 'https://github.com/claudioredi/aerogear-ios-http.git'
	pod 'AeroGearOAuth2', :git => 'https://github.com/claudioredi/aerogear-ios-oauth2.git', :branch => 'openstack'
	pod 'SwiftSpinner' 
	pod 'GoogleMaps'
	pod 'ImageSlideshow', '~> 0.3'
	pod 'Cosmos', '~> 1.1'
	pod 'AHKActionSheet'
	pod 'MLPAutoCompleteTextField'
	pod 'AMTagListView'
	pod 'Parse'
	pod 'Fabric'
	pod 'Crashlytics'
end

